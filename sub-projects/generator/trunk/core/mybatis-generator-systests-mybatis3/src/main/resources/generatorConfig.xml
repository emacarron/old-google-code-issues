<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="FlatJava5" defaultModelType="flat">
    <property name="suppressTypeWarnings" value="true" /> <!-- should be ignored -->
    
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

    <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
        connectionURL="jdbc:hsqldb:mem:aname"
        userId="sa" />

    <javaModelGenerator targetPackage="mbg.test.mb3.generated.flat.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mbg.test.mb3.generated.flat.xml"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="mbg.test.mb3.generated.flat.mapper"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table tableName="FieldsOnly" />
    <table tableName="PKOnly" />
    <table tableName="PKFields" alias="B" />
    <table tableName="PKBlobs" />
    <table tableName="PKFieldsBlobs" />
    <table tableName="FieldsBlobs" />
    <table tableName="awful table" alias="A">
      <property name="useColumnIndexes" value="true"/>
      <generatedKey column="CuStOmEr iD" sqlStatement="HSQLDB" identity="true" />
      <columnOverride column="first name" property="firstFirstName" />
      <columnOverride column="first_name" property="secondFirstName" />
      <columnOverride column="firstName" property="thirdFirstName" />
      <columnOverride column="from" delimitedColumnName="true" >
        <property name="fredswife" value="wilma"/>
      </columnOverride>
      <columnOverride column="active" javaType="boolean" />
    </table>
  </context>

  <context id="HierarchicalJava5" targetRuntime="MyBatis3" defaultModelType="hierarchical">
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

    <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
        connectionURL="jdbc:hsqldb:mem:aname"
        userId="sa" />

    <javaModelGenerator targetPackage="mbg.test.mb3.generated.hierarchical.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mbg.test.mb3.generated.hierarchical.xml"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="mbg.test.mb3.generated.hierarchical.mapper"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table tableName="FieldsOnly" />
    <table tableName="PKOnly" />
    <table tableName="PKFields" alias="B" />
    <table tableName="PKBlobs" />
    <table tableName="PKFieldsBlobs" />
    <table tableName="FieldsBlobs" />
    <table tableName="awful table" alias="A">
      <property name="useColumnIndexes" value="true"/>
      <generatedKey column="CuStOmEr iD" sqlStatement="HSQLDB" identity="true" />
      <columnOverride column="first name" property="firstFirstName" />
      <columnOverride column="first_name" property="secondFirstName" />
      <columnOverride column="firstName" property="thirdFirstName" />
      <columnOverride column="from" delimitedColumnName="true" />
      <columnOverride column="active" javaType="boolean" />
    </table>
  </context>

  <context id="ConditionalJava5" targetRuntime="MyBatis3" defaultModelType="conditional">
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
    <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

    <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
        connectionURL="jdbc:hsqldb:mem:aname"
        userId="sa" />

    <javaModelGenerator targetPackage="mbg.test.mb3.generated.conditional.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mbg.test.mb3.generated.conditional.xml"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="mbg.test.mb3.generated.conditional.mapper"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table tableName="FieldsOnly" />
    <table tableName="PKOnly" />
    <table tableName="PKFields" alias="B" />
    <table tableName="PKBlobs" />
    <table tableName="PKFieldsBlobs" />
    <table tableName="FieldsBlobs" />
    <table tableName="awful table" alias="A">
      <property name="useColumnIndexes" value="true"/>
      <generatedKey column="CuStOmEr iD" sqlStatement="HSQLDB" identity="true" />
      <columnOverride column="first name" property="firstFirstName" />
      <columnOverride column="first_name" property="secondFirstName" />
      <columnOverride column="firstName" property="thirdFirstName" />
      <columnOverride column="from" delimitedColumnName="true" />
      <columnOverride column="active" javaType="boolean" />
    </table>
  </context>

  <context id="miscellaneousTests" targetRuntime="MyBatis3">
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
    <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
    <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin" >
      <property name="searchString" value="Example$"/>
      <property name="replaceString" value="Criteria"/>
    </plugin>

    <commentGenerator>
      <property name="suppressDate" value="true"/>
    </commentGenerator>
    
    <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
        connectionURL="jdbc:hsqldb:mem:aname"
        userId="sa" />

    <javaModelGenerator targetPackage="mbg.test.mb3.generated.miscellaneous.model" targetProject="MAVEN">
      <property name="trimStrings" value="true" />
      <property name="rootClass" value="mbg.test.common.BaseClass" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mbg.test.mb3.generated.miscellaneous.xml"  targetProject="MAVEN">
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="mbg.test.mb3.generated.miscellaneous.mapper"  targetProject="MAVEN">
      <property name="rootInterface" value="mbg.test.common.BaseInterface" />
      <property name="methodNameCalculator" value="EXTENDED"/>
    </javaClientGenerator>

    <table tableName="BlobsOnly" /> <!-- should generate a warning -->
    <table tableName="NonExistantTable" /> <!-- should generate a warning -->
    <table tableName="PKFields" domainObjectName="MyObject" alias="A" >
      <ignoreColumn column="fred" /> <!-- should generate a warning -->
      <ignoreColumn column="decimal30Field" />
      <columnOverride column="datefield" property="startDate"/>
      <columnOverride column="decimal155Field" javaType="java.lang.Double" />
      <columnOverride column="decimal60Field" javaType="int" />
      <columnOverride column="firstName" javaType="mbg.test.common.FirstName"
         typeHandler="mbg.test.mb3.common.FirstNameTypeHandler"/>
      <columnOverride column="timeField" javaType="mbg.test.common.MyTime"
         typeHandler="mbg.test.mb3.common.MyTimeTypeHandler"/>
    </table>
    <table tableName="RegexRename">
      <generatedKey column="CUST_ID" sqlStatement="call next value for TestSequence" type="pre" />
      <columnRenamingRule searchString="^CUST" />
    </table>
    <table tableName="EnumTest">
      <columnOverride column="name" javaType="mbg.test.mb3.common.TestEnum"/>
    </table>
  </context>
  
  <context id="miscellaneousTests_NoClient" targetRuntime="MyBatis3">
    <property name="autoDelimitKeywords" value="true" />
    
    <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
        connectionURL="jdbc:hsqldb:mem:aname"
        userId="sa" />

    <javaModelGenerator targetPackage="mbg.test.mb3.generated.miscellaneous.model" targetProject="MAVEN">
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mbg.test.mb3.generated.miscellaneous.xml"
                     targetProject="MAVEN"/>

    <table tableName="AnotherAwfulTable" />
    <table tableName="NameView" />
  </context>
</generatorConfiguration>
